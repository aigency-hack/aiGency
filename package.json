{
  "name": "next-skeleton",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install && cp -n .env.example .env || true",
    "tsc:check": "tsc --noEmit",
    "lint:check": "eslint \"{src,pages}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"{src,pages}/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"{src,pages}/**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"{src,pages}/**/*.{ts,tsx}\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate:backend": "swagger-to-ts http://localhost:5002/doc-json --output src/generated/schema.ts --shallow"
